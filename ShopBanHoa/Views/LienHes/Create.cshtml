@model ShopBanHoa.Models.LienHe

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="login">
        <div class="wrap">
            <ul class="breadcrumb breadcrumb__t"><a class="home" href="#">Home</a>  / Liên Hệ </ul>
            <div class="content-top">
                <div class="form-horizontal">
                    <form method="post" action="contact-post.html">
                        <div class="to">
                            @Html.EditorFor(model => model.TenKH, new { htmlAttributes = new { @class = "form-control", @placeholder = "Họ tên" } })
                            @Html.EditorFor(model => model.DiaChi, new { htmlAttributes = new { @class = "form-control", @placeholder = "Địa chỉ" } })
                        </div>
                        <div class="to">
                            @Html.EditorFor(model => model.SDT, new { htmlAttributes = new { @class = "form-control", @placeholder = "SĐT" } })
                            @Html.EditorFor(model => model.Email, new { htmlAttributes = new { @class = "form-control", @placeholder = "Email" } })
                        </div>
                        <div class="to">
                            @Html.EditorFor(model => model.NgayCapNhat, new { htmlAttributes = new { @class = "form-control", @placeholder = @DateTime.Now.ToShortDateString(), @disabled = "" } })
                        </div>
                        <div class="text">
                            @Html.EditorFor(model => model.NoiDung, new { htmlAttributes = new { @class = "form-control", @placeholder = "Nội dung" } })
                        </div>
                        <div class="submit">
                            <input type="submit" value="Create">
                        </div>
                    </form>
                </div>
                <div class="map">
                    <li><iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d1307.8505995406033!2d106.71436052773808!3d10.801847213585102!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x317528a459cb43ab%3A0x6c3d29d370b52a7e!2sHUTECH+University!5e0!3m2!1sen!2s!4v1524045015374" width="100%" height="200" frameborder="0" style="border:0" allowfullscreen></iframe></li>
                </div>
            </div>
        </div>
    </div>

}